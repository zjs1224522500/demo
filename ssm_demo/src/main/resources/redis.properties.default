# redis对应的服务器IP地址
redis.host=127.0.0.1
# redis运行的端口号
redis.port=6379
# redis的password
redis.password=root


# 最大连接数：能够同时建立的“最大链接个数”
redis.maxActive=200
# 最大空闲数：空闲链接数大于maxIdle时，将进行回收
redis.maxIdle=20
# 最小空闲数：低于minIdle时，将创建新的链接
redis.minIdle=5
# 最大等待时间：单位ms
redis.maxWait=1000
# 使用连接时，检测连接是否成功
redis.testOnBorrow=true
# 返回连接时，检测连接是否成功
redis.testOnReturn=true
# 超时时间：单位ms
redis.timeout=100000
# 缓存默认过期时间 单位ms
defaultCacheExpireTime=3600